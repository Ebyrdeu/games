<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="635.0" prefWidth="600.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.games.controller.PVEController">

    <fx:include fx:id="topBar" source="layout-view.fxml" />
    <FlowPane id="board" layoutY="35.0" prefHeight="600.0" prefWidth="600.0">
        <Button fx:id="btn1" prefHeight="200" prefWidth="200" />
        <Button fx:id="btn2" prefHeight="200" prefWidth="200" />
        <Button fx:id="btn3" prefHeight="200" prefWidth="200" />
        <Button fx:id="btn4" prefHeight="200" prefWidth="200" />
        <Button fx:id="btn5" prefHeight="200" prefWidth="200" />
        <Button fx:id="btn6" prefHeight="200" prefWidth="200" />
        <Button fx:id="btn7" prefHeight="200" prefWidth="200" />
        <Button fx:id="btn8" prefHeight="200" prefWidth="200" />
        <Button fx:id="btn9" prefHeight="200" prefWidth="200" />
    </FlowPane>
   <VBox fx:id="restart" visible="false" alignment="CENTER" layoutY="35.0" prefHeight="600.0" prefWidth="600.0">
         <Button id="restartBtn" mnemonicParsing="false" onAction="#onRestart" text="Restart Game" />
   </VBox>
</Pane>
